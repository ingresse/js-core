{
  "name": "@ingresse/sdk",
  "version": "0.1.1",
  "description": "Ingresse plataform Javascript SDK",
  "main": "dist/sdk.js",
  "module": "src/index.js",
  "keywords": [
    "ingresse",
    "sdk"
  ],
  "author": "Ingresse <frontend@ingresse.com>",
  "homepage": "https://ingresse.github.io/js-core",
  "license": "MIT",
  "directories": {
    "lib": "src"
  },
  "files": [
    "dist",
    "src"
  ],
  "publishConfig": {
    "access": "public"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/ingresse/js-core.git"
  },
  "bugs": {
    "url": "https://github.com/ingresse/js-core/issues"
  },
  "scripts": {
    "build": "webpack",
    "unit": "jest --collectCoverage --no-cache --watch",
    "test": "CI=1 jest --collectCoverage --no-cache",
    "predev": "cd docs && npm i && clear",
    "dev": "cd docs && npm start",
    "deploy": "npm run build && npm publish"
  },
  "dependencies": {
    "@ingresse/request": "0.0.5"
  },
  "devDependencies": {
    "@babel/core": "^7.9.0",
    "@babel/plugin-transform-modules-commonjs": "^7.9.0",
    "@babel/preset-env": "^7.9.0",
    "babel-loader": "^8.1.0",
    "dayjs": "^1.8.30",
    "jest": "^25.2.4",
    "prettier": "^2.1.2",
    "webpack": "^4.42.1",
    "webpack-cli": "^3.3.11"
  },
  "jest": {
    "automock": false,
    "setupFiles": [
      "./setup.jest.js"
    ]
  }
}
