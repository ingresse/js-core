{
  "name": "@ingresse/sdk",
  "version": "0.5.0",
  "description": "Ingresse plataform Javascript SDK",
  "keywords": [
    "ingresse",
    "sdk"
  ],
  "author": "Ingresse <frontend@ingresse.com>",
  "homepage": "https://ingresse.github.io/js-core",
  "license": "MIT",
  "directories": {
    "lib": "src"
  },
  "files": [
    "dist",
    "src"
  ],
  "publishConfig": {
    "access": "public"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/ingresse/js-core.git"
  },
  "bugs": {
    "url": "https://github.com/ingresse/js-core/issues"
  },
  "type": "module",
  "main": "./dist/tools.js",
  "module": "./dist/tools.module.js",
  "source": "src/index.js",
  "exports": "./dist/tools.modern.js",
  "unpkg": "./dist/tools.js",
  "scripts": {
    "dev": "microbundle-crl watch",
    "dev:test": "jest --collectCoverage --watchAll",
    "test": "jest --collectCoverage",
    "build": "rm -rf dist && microbundle-crl",
    "deploy": "npm run build && npm publish"
  },
  "dependencies": {
    "@fnando/cnpj": "^1.0.2",
    "@fnando/cpf": "^1.0.2",
    "@ingresse/request": "0.0.5",
    "broleto": "0.0.10",
    "dayjs": "^1.9.1"
  },
  "devDependencies": {
    "@babel/core": "^7.9.0",
    "@babel/plugin-transform-modules-commonjs": "^7.9.0",
    "@babel/preset-env": "^7.9.0",
    "babel-loader": "^8.1.0",
    "jest": "^25.2.4",
    "microbundle-crl": "^0.13.11",
    "prettier": "^2.1.2"
  },
  "jest": {
    "automock": false,
    "setupFiles": [
      "./setup.jest.js"
    ]
  }
}
